{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {
    "fhir2": "^1.2.0",
    "webservices.rest": "^2.24.0"
  },
  "pages": [
  ],
  "extensions": [
    {
      "name": "clinical-views-divider",
      "component": "clinicalViewsDivider",
      "slot": "patient-chart-dashboard-slot",
      "order": 15
    },
    {
      "name": "all-encounters",
      "component": "encountersDashboardLink",
      "slot": "patient-chart-dashboard-slot",
      "meta": {
        "slot": "patient-chart-all-encounters-dashboard-slot",
        "columns": 1,
        "path": "Encounters"
      },
      "order": 13
    },
    {
      "name": "encounters-view",
      "slot": "patient-chart-all-encounters-dashboard-slot",
      "component": "allEncounters",
      "meta": {
        "columnSpan": 1
      },
      "order": 13
    },
    {
      "name": "care-and-treatment-link",
      "slot": "patient-chart-dashboard-slot",
      "component": "careAndTreatmentDashboardLink",
      "order": 16,
      "meta": {
        "slot": "care-and-treatment-slot",
        "columns": 1,
        "path": "care-and-treatment",
        "layoutMode": "anchored"
      }
    },
    {
      "name": "care-and-treatment-ext",
      "slot": "care-and-treatment-slot",
      "component": "careAndTreatmentDashboard"
    }
  ]
}
